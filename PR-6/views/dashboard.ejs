        overflow: hidden;
    }

    .gallery-main img {
        width: 100%;
        transition: transform 0.3s ease-in-out;
    }

    .gallery-main .col-2::before {
        content: "";
        position: absolute;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background: rgba(0, 0, 0, 0.5);
        opacity: 0;
        transition: opacity 0.3s ease-in-out;
    }

    .gallery-main .col-2:hover::before {
        opacity: 1;
    }

    .gallery-main .col-2:hover img {
        transform: scale(1.1);
        transition: 0.9s;
    }
    .footer{
