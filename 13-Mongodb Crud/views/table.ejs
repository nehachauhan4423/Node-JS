<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div>
        <h1>View USer</h1>
        <table>
            <thead>
                <tr>
                    <th>Id</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Password</th>
                </tr>
            </thead>
            <tbody>
                <% record.mao((val)=>{ %>
                    <tr>
                        <td>
                            <%= val.id %>
                        </td>
                        <td>
                            <%= val.username %>
                        </td>
                        <td>
                            <%= val.useremail %>
                        </td>
                        <td>
                            <%= val.userpassword %>
                        </td>
                        <td>
                            <a href="/deleteuser?deleteId=<% val.id %>">Delete</a>
                            <a href="/edituser?editId=<% val.id %>">Edit</a>
                        </td>
                    </tr>
               <% }) %>
            </tbody>
        </table>
        <a href="/">Add</a>
    </div>
</body>
</html>